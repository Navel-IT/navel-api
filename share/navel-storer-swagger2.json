{
    "swagger": "2.0",
    "info": {
        "title": "navel-storer",
        "description": "https://github.com/navel-it/navel-storer",
        "version": "0.1.0",
        "contact": {
            "name": "Yoann Le Garff, Nicolas Boquet and Yann Le Bras"
        },
        "license": {
            "name": "Apache License, Version 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        }
    },
    "basePath": "/api",
    "schemes": [
        "http",
        "https"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/status": {
            "get": {
                "summary": "Show product status",
                "operationId": "show_status",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Product",
                "tags": [
                    "status"
                ],
                "responses": {
                    "200": {
                        "description": "Product status",
                        "schema": {
                            "$ref": "#/definitions/productStatus"
                        }
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/save_configuration": {
            "put": {
                "summary": "Save all configuration",
                "operationId": "save_all_configuration",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Backup",
                "tags": [
                    "saveConfiguration"
                ],
                "responses": {
                    "200": {
                        "$ref": "#/responses/oKKO"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/meta": {
            "get": {
                "summary": "Show meta configuration",
                "operationId": "show_meta",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Meta",
                "tags": [
                    "meta"
                ],
                "responses": {
                    "200": {
                        "description": "Meta configuration",
                        "schema": {
                            "$ref": "#/definitions/meta"
                        }
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/jobs": {
            "get": {
                "summary": "List job types",
                "operationId": "list_job_types",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Job",
                "tags": [
                    "job"
                ],
                "responses": {
                    "200": {
                        "description": "Job types",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": [
                                    "string",
                                    "integer",
                                    "number"
                                ]
                            }
                        }
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/jobs/{jobType}": {
            "get": {
                "summary": "List job names by job type",
                "operationId": "list_jobs_by_type",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Job",
                "tags": [
                    "job"
                ],
                "parameters": [{
                    "$ref": "#/parameters/jobType"
                }],
                "responses": {
                    "200": {
                        "description": "Job names by job type",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": [
                                    "string",
                                    "integer",
                                    "number"
                                ]
                            }
                        }
                    },
                    "404": {
                        "$ref": "#/responses/resourceCouldNotBeFound"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/jobs/{jobType}/{jobName}": {
            "get": {
                "summary": "Show the properties of a job",
                "operationId": "show_job_by_type_and_name",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Job",
                "tags": [
                    "job"
                ],
                "parameters": [{
                    "$ref": "#/parameters/jobType"
                }, {
                    "$ref": "#/parameters/jobName"
                }],
                "responses": {
                    "200": {
                        "description": "Job",
                        "schema": {
                            "$ref": "#/definitions/job"
                        }
                    },
                    "404": {
                        "$ref": "#/responses/resourceCouldNotBeFound"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/jobs/{jobType}/{jobName}/enable": {
            "put": {
                "summary": "Enable a job",
                "operationId": "enable_job_by_type_and_name",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Job",
                "tags": [
                    "job"
                ],
                "parameters": [{
                    "$ref": "#/parameters/jobType"
                }, {
                    "$ref": "#/parameters/jobName"
                }],
                "responses": {
                    "200": {
                        "$ref": "#/responses/oKKO"
                    },
                    "404": {
                        "$ref": "#/responses/resourceCouldNotBeFound"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/jobs/{jobType}/{jobName}/disable": {
            "put": {
                "summary": "Disable a job",
                "operationId": "disable_job_by_type_and_name",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Job",
                "tags": [
                    "job"
                ],
                "parameters": [{
                    "$ref": "#/parameters/jobType"
                }, {
                    "$ref": "#/parameters/jobName"
                }],
                "responses": {
                    "200": {
                        "$ref": "#/responses/oKKO"
                    },
                    "404": {
                        "$ref": "#/responses/resourceCouldNotBeFound"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/jobs/{jobType}/{jobName}/execute": {
            "put": {
                "summary": "Execute a job",
                "operationId": "execute_job_by_type_and_name",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::Job",
                "tags": [
                    "job"
                ],
                "parameters": [{
                    "$ref": "#/parameters/jobType"
                }, {
                    "$ref": "#/parameters/jobName"
                }],
                "responses": {
                    "200": {
                        "$ref": "#/responses/oKKO"
                    },
                    "404": {
                        "$ref": "#/responses/resourceCouldNotBeFound"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/webservices": {
            "get": {
                "summary": "List webservice names",
                "operationId": "list_webservices",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::WebService",
                "tags": [
                    "webservice"
                ],
                "responses": {
                    "200": {
                        "description": "Web service names",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": [
                                    "string",
                                    "integer",
                                    "number"
                                ]
                            }
                        }
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        },
        "/webservices/{webServiceName}": {
            "get": {
                "summary": "Show the properties of a web service",
                "operationId": "show_webservice",
                "x-mojo-controller": "Navel::Scheduler::Mojolicious::Application::Controller::Swagger2::WebService",
                "tags": [
                    "webservice"
                ],
                "parameters": [{
                    "$ref": "#/parameters/webServiceName"
                }],
                "responses": {
                    "200": {
                        "description": "Web service",
                        "schema": {
                            "$ref": "#/definitions/webservice"
                        }
                    },
                    "404": {
                        "$ref": "#/responses/resourceCouldNotBeFound"
                    },
                    "default": {
                        "$ref": "#/responses/unexpectedError"
                    }
                }
            }
        }
    },
    "definitions": {
        "productStatus": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "version": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "api_version": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                }
            }
        },
        "meta": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "collectors",
                "publishers",
                "webservices"
            ],
            "properties": {
                "collectors": {
                    "$ref": "#/definitions/metaCollectors"
                },
                "publishers": {
                    "$ref": "#/definitions/metaPublisher"
                },
                "webservices": {
                    "$ref": "#/definitions/metaWebServices"
                }
            }
        },
        "metaCollectors": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "definitions_from_file",
                "maximum"
            ],
            "properties": {
                "definitions_from_file": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "maximum": {
                    "type": "integer",
                    "minimum": 0
                }
            }
        },
        "metaPublisher": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "definitions_from_file",
                "maximum"
            ],
            "properties": {
                "definitions_from_file": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "maximum": {
                    "type": "integer",
                    "minimum": 0
                }
            }
        },
        "metaWebServices": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "definitions_from_file",
                "credentials",
                "mojo_server"
            ],
            "properties": {
                "definitions_from_file": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "credentials": {
                    "$ref": "#/definitions/metaWebServicesCredentials"
                },
                "mojo_server": {
                    "$ref": "#/definitions/metaWebServicesMojoServer"
                }
            }
        },
        "metaWebServicesCredentials": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "login",
                "password"
            ],
            "properties": {
                "login": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "password": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                }
            }
        },
        "metaWebServicesMojoServer": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "reverse_proxy": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 1
                },
                "backlog": {
                    "type": "integer"
                },
                "inactivity_timeout": {
                    "type": "integer"
                },
                "max_clients": {
                    "type": "integer"
                },
                "max_requests": {
                    "type": "integer"
                }
            }
        },
        "job": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "name",
                "type",
                "enabled",
                "singleton",
                "running"
            ],
            "properties": {
                "name": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "type": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "enabled": {
                    "type": [
                        "integer",
                        "boolean"
                    ],
                    "minimum": 0,
                    "maximum": 1
                },
                "singleton": {
                    "type": [
                        "integer",
                        "boolean"
                    ],
                    "minimum": 0,
                    "maximum": 1
                },
                "running": {
                    "type": "integer",
                    "minimum": 0
                }
            }
        },
        "webservice": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "name",
                "interface_mask",
                "port",
                "tls",
                "ca",
                "cert",
                "ciphers",
                "key",
                "verify"
            ],
            "properties": {
                "name": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "interface_mask": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "port": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 65535
                },
                "tls": {
                    "type": [
                        "integer",
                        "boolean"
                    ],
                    "minimum": 0,
                    "maximum": 1
                },
                "ca": {
                    "type": [
                        "null",
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "cert": {
                    "type": [
                        "null",
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "ciphers": {
                    "type": [
                        "null",
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "key": {
                    "type": [
                        "null",
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "verify": {
                    "type": [
                        "null",
                        "string",
                        "integer",
                        "number"
                    ]
                }
            }
        },
        "oKKO": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "ok",
                "ko"
            ],
            "properties": {
                "ok": {
                    "type": "array",
                    "items": {}
                },
                "ko": {
                    "type": "array",
                    "items": {}
                }
            }
        }
    },
    "parameters": {
        "credentials": {
            "name": "credentials",
            "in": "body",
            "required": true,
            "schema": {
                "$ref": "#/definitions/metaWebServicesCredentials"
            }
        },
        "modifiedProperties": {
            "name": "modifiedProperties",
            "in": "body",
            "required": true,
            "schema": {
                "type": "object",
                "properties": {}
            }
        },
        "jobType": {
            "name": "jobType",
            "in": "path",
            "required": true,
            "type": "string"
        },
        "jobName": {
            "name": "jobName",
            "in": "path",
            "required": true,
            "type": "string"
        },
        "webServiceName": {
            "name": "webServiceName",
            "in": "path",
            "required": true,
            "type": "string"
        }
    },
    "responses": {
        "oKKO": {
            "description": "OK and KO messages",
            "schema": {
                "$ref": "#/definitions/oKKO"
            }
        },
        "badRequest": {
            "description": "Incorrect request",
            "schema": {
                "$ref": "#/definitions/oKKO"
            }
        },
        "resourceCouldNotBeFound": {
            "description": "This resource could not be found",
            "schema": {
                "$ref": "#/definitions/oKKO"
            }
        },
        "resourceAlreadyExists": {
            "description": "This resource already exists",
            "schema": {
                "$ref": "#/definitions/oKKO"
            }
        },
        "unexpectedError": {
            "description": "Unexpected Error",
            "schema": {
                "$ref": "#/definitions/oKKO"
            }
        }
    }
}
