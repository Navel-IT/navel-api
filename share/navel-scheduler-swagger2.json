            "type": "object",
            "required": [
                "login",
                "password"
            ],
            "properties": {
                "login": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "password": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                }
            }
        },
        "mainWebServicesMojoServer": {
            "type": "object",
            "properties": {
                "reverse_proxy": {
                    "type": "integer",
                    "minimum": 0
                },
                "backlog": {
                    "type": "integer",
                    "minimum": 0
                },
                "inactivity_timeout": {
                    "type": "integer",
                    "minimum": 0
                },
                "max_clients": {
                    "type": "integer",
                    "minimum": 0
                },
                "max_requests": {
                    "type": "integer",
                    "minimum": 0
                },
                "accepts": {
                    "type": "integer",
                    "minimum": 0
                },
                "accept_interval": {
                    "type": "number",
                    "minimum": 0
                },
                "graceful_timeout": {
                    "type": "number",
                    "minimum": 0
                },
