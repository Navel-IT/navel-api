                "definitions_from_file",
                "credentials",
                "mojo_server"
            ],
            "properties": {
                "definitions_from_file": {
                    "type": [
                        "string",
                        "integer",
                        "number"
                    ]
                },
                "credentials": {
                    "$ref": "#/definitions/mainWebServicesCredentials"
                },
                "mojo_server": {
                    "$ref": "#/definitions/mainWebServicesMojoServer"
                }
            }
        },
