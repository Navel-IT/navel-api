                },
                "enabled": {
                    "type": [
                        "integer",
                        "boolean"
                    ],
                    "minimum": 0,
                    "maximum": 1
                },
                "singleton": {
                    "type": [
                        "integer",
                        "boolean"
                    ],
                    "minimum": 0,
